%img.gameBG{src: "/clickdummy/w2.jpg", width: "100%"}

:coffeescript
    $ ->
        bg = $(".gameBG")[0]
        $(document).click (event)->
            if event.offsetY < $(document).height()/2
                console.log "Click upper half"
            else
                console.log "Click lower half"
                if bg.src.includes("w2")
                    bg.src = "/clickdummy/w3.jpg"
                else if bg.src.includes("w3")
                    bg.src = "/clickdummy/w4.jpg"
                else 
                    window.location.href = "board"